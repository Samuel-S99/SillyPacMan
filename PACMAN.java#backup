            if(pinkghostx-20==0 && pinkghostx+20<=40 && pinkghosty==320 && pinkghosty+40<=360){
                gowhere=(int)(Math.random()*3+1);

                if(gowhere==1){
                    pinkxInc=2;
                    pinkyInc=0;

                }
                else if(gowhere==2){
                    pinkxInc=0;
                    pinkyInc=2;

                }
                else if(gowhere==3){
                    pinkxInc=0;
                    pinkyInc=2;

                }
                
             
            }
            if(pinkghostx-20==100 && pinkghostx+20<=140 && pinkghosty==320 && pinkghosty+40<=360){
                gowhere=(int)(Math.random()*4+1);

                if(gowhere==1){
                    pinkxInc=2;
                    pinkyInc=0;

                }
                else if(gowhere==2){
                    pinkxInc=0;
