            if(pinkghostx-20==100 && pinkghostx+20<=140 && pinkghosty==120 && pinkghosty+40<=160){
                gowhere=(int)(Math.random()*4+1);

                if(gowhere==1){
                    pinkxInc=2;
                    pinkyInc=0;

                }
                else if(gowhere==2){
                    pinkxInc=0;
                    pinkyInc=2;

                }
                else if(gowhere==3){
                    pinkxInc=-2;
                    pinkyInc=0;

                }
                else if(gowhere==4){
                    pinkxInc=0;
                    pinkyInc=-2;

                }
            
            }
            if(pinkghostx-20==220 && pinkghostx+20<=260 && pinkghosty==120 && pinkghosty+40<=160){
                gowhere=(int)(Math.random()*4+1);

                if(gowhere==1){
                    pinkxInc=2;
                    pinkyInc=0;

